@prefix : <http://votes.examples.com/ontology/> .
@prefix v: <http://votes.examples.com/ontology/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://votes.examples.com/ontology/> .

<http://votes.examples.com/ontology/> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://votes.examples.com/ontology/abstains
v:abstains rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf v:reactsToVote .


###  http://votes.examples.com/ontology/appliesTo
v:appliesTo rdf:type owl:ObjectProperty ;
            rdfs:domain v:Bill ;
            rdfs:range v:Polity ;
            rdfs:label "appliesTo"@en .


###  http://votes.examples.com/ontology/hasText
v:hasText rdf:type owl:ObjectProperty ;
          rdfs:domain v:Bill ;
          rdfs:range [ rdf:type owl:Restriction ;
                       owl:onProperty <http:www.example.com/ontology/o_00111> ;
                       owl:someValuesFrom xsd:string
                     ] .


###  http://votes.examples.com/ontology/legalizesFor
v:legalizesFor rdf:type owl:ObjectProperty ;
               rdfs:domain v:VotingAssembly ;
               rdfs:range v:Polity ;
               rdfs:comment "The polity that a voting assembly's can pass legislation onto." ;
               rdfs:label "legalizesFor"@en .


###  http://votes.examples.com/ontology/memberOf
v:memberOf rdf:type owl:ObjectProperty ;
           rdfs:domain v:Voter ;
           rdfs:range v:Party ;
           rdfs:label "memberOf"@en .


###  http://votes.examples.com/ontology/processed
v:processed rdf:type owl:ObjectProperty ;
            owl:inverseOf v:processedBy ;
            rdfs:domain v:VotingAssembly ;
            rdfs:range v:Bill ;
            rdfs:label "processed"@en .


###  http://votes.examples.com/ontology/processedBy
v:processedBy rdf:type owl:ObjectProperty ;
              rdfs:domain v:Bill ;
              rdfs:range v:VotingAssembly ;
              rdfs:label "processedBy"@en .


###  http://votes.examples.com/ontology/reactsToVote
v:reactsToVote rdf:type owl:ObjectProperty ;
               rdfs:domain v:Voter ;
               rdfs:range v:Bill .


###  http://votes.examples.com/ontology/votesNay
v:votesNay rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf v:votesOn .


###  http://votes.examples.com/ontology/votesOn
v:votesOn rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf v:reactsToVote .


###  http://votes.examples.com/ontology/votesYay
v:votesYay rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf v:votesOn .


###  http://www.co-ode.org/ontologies/ont.owl#o_00105
<http://www.co-ode.org/ontologies/ont.owl#o_00105> rdf:type owl:ObjectProperty ;
                                                   rdfs:domain v:Voter ;
                                                   rdfs:range v:VotingAssembly ;
                                                   rdfs:label "votesIn"@en .


#################################################################
#    Data properties
#################################################################

###  http://xmlns.com/foaf/0.1/age
foaf:age rdf:type owl:DatatypeProperty .


###  http:www.example.com/ontology/o_00111
<http:www.example.com/ontology/o_00111> rdf:type owl:DatatypeProperty ;
                                        rdfs:label "bill_text"@en .


#################################################################
#    Classes
#################################################################

###  http://votes.examples.com/ontology/Agent
v:Agent rdf:type owl:Class ;
        owl:equivalentClass foaf:Agent ;
        rdfs:label "Agent"@en .


###  http://votes.examples.com/ontology/Bill
v:Bill rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty v:processedBy ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass v:VotingAssembly
                           ] .


###  http://votes.examples.com/ontology/Commune
v:Commune rdf:type owl:Class ;
          rdfs:subClassOf v:Polity ;
          rdfs:label "Commune"@en .


###  http://votes.examples.com/ontology/County
v:County rdf:type owl:Class ;
         rdfs:subClassOf v:Polity ;
         rdfs:label "County"@en .


###  http://votes.examples.com/ontology/Federation
v:Federation rdf:type owl:Class ;
             rdfs:subClassOf v:Polity ;
             rdfs:label "Federation"@en .


###  http://votes.examples.com/ontology/FemaleVoter
v:FemaleVoter rdf:type owl:Class ;
              rdfs:subClassOf v:Voter ;
              rdfs:label "FemaleVoter"@en .


###  http://votes.examples.com/ontology/MaleVoter
v:MaleVoter rdf:type owl:Class ;
            rdfs:subClassOf v:Voter ;
            rdfs:label "MaleVoter"@en .


###  http://votes.examples.com/ontology/MiddleAgedVoter
v:MiddleAgedVoter rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( v:Voter
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty foaf:age ;
                                                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                    owl:onDatatype xsd:integer ;
                                                                                    owl:withRestrictions ( [ xsd:minInclusive 40
                                                                                                           ]
                                                                                                           [ xsd:maxExclusive 60
                                                                                                           ]
                                                                                                         )
                                                                                  ]
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf v:Voter ;
                  rdfs:label "MiddleAgedVoter"@en .


###  http://votes.examples.com/ontology/OldVoter
v:OldVoter rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty foaf:age ;
                                 owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                      owl:onDatatype xsd:integer ;
                                                      owl:withRestrictions ( [ xsd:minInclusive 60
                                                                             ]
                                                                           )
                                                    ]
                               ] ;
           rdfs:subClassOf v:Voter ;
           rdfs:label "OldVoter"@en .


###  http://votes.examples.com/ontology/Party
v:Party rdf:type owl:Class ;
        rdfs:subClassOf v:Agent ;
        rdfs:comment "A collection of relatively likeminded Voters" ;
        rdfs:label "Party"@en .


###  http://votes.examples.com/ontology/Polity
v:Polity rdf:type owl:Class ;
         rdfs:subClassOf v:Agent ;
         rdfs:comment "Something onto which laws are applied" ;
         rdfs:label "Polity"@en .


###  http://votes.examples.com/ontology/State
v:State rdf:type owl:Class ;
        rdfs:subClassOf v:Polity ;
        rdfs:label "State"@en .


###  http://votes.examples.com/ontology/Voter
v:Voter rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty v:reactsToVote ;
                                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onClass v:Bill
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty <http://www.co-ode.org/ontologies/ont.owl#o_00105> ;
                                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onClass v:VotingAssembly
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf v:Agent .


###  http://votes.examples.com/ontology/VotingAssembly
v:VotingAssembly rdf:type owl:Class ;
                 rdfs:subClassOf v:Agent ;
                 rdfs:label "VotingAssembly"@en .


###  http://votes.examples.com/ontology/YoungVoter
v:YoungVoter rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty foaf:age ;
                                   owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                        owl:onDatatype xsd:integer ;
                                                        owl:withRestrictions ( [ xsd:maxExclusive 40
                                                                               ]
                                                                             )
                                                      ]
                                 ] ;
             rdfs:subClassOf v:Voter ;
             rdfs:label "YoungVoter"@en .


###  http://xmlns.com/foaf/0.1/Agent
foaf:Agent rdf:type owl:Class .


